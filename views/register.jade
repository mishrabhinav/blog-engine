extends layout

block page
  - var menu = 'register'

block content
  .col-md-4.col-md-offset-4
    h2 Register
    div= error
    div
      form(action="/register", method="POST")
        p
          input.form-control(name="email", type="text", placeholder="Email ID")
        p
          input.form-control(name="password", type="password", placeholder="Password")
        p
          button.btn.btn-lg.btn-primary.btn-block(type="submit") Register
